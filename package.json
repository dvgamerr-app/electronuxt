{
  "name": "electronuxt",
  "version": "1.0.0",
  "description": "Create an Electron app that serves a Nuxt.js app",
  "main": "main.js",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/touno-io/electronuxt.git"
  },
  "author": "Kananek Thongkam <info@touno.io>",
  "license": "MIT",
  "scripts": {
    "dev2": "nuxt",
    "dev": "electron main.js",
    "build": "nuxt generate && electron-builder",
    "start": "electron main.js"
  },
  "dependencies": {},
  "devDependencies": {
    "electron": "^4.1.4",
    "electron-builder": "^20.39.0",
    "nodemon": "^1.18.11",
    "nuxt": "^2.6.1"
  },
  "bugs": {
    "url": "https://github.com/touno-io/electronuxt/issues"
  },
  "homepage": "https://github.com/touno-io/electronuxt#readme",
  "keywords": [
    "electronjs",
    "nuxtjs",
    "vuejs",
    "window",
    "app",
    "javascript"
  ],
  "build": {
    "productName": "hentai-downloader",
    "appId": "hentai.downloader.touno.io",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/**/*"
    ],
    "mac": {
      "icon": "build/icons/icon.icns"
    },
    "win": {
      "icon": "build/icons/icon.ico"
    },
    "linux": {
      "icon": "build/icons"
    }
  }
}
